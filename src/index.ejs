						"@type": "PostalAddress",
						"addressCountry": {
							"@type": "Country",
							"name": "日本"
						},
						"postalCode": "543-0001",
						"addressRegion": "大阪府大阪市天王寺区",
						"streetAddress": "上本町6-8-4"
					}
